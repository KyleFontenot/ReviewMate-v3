<script>
  function deferAddimage(){
    const introbg = document.querySelector('#bgimage')?.style;
    introbg.backgroundImage = `url('/marc-mueller.webp'), url('/marc-mueller.jpeg')`;
    introbg.background = `url('/marc-mueller.webp'), url('/marc-mueller.jpeg')`;
    introbg.backgroundRepeat = `no-repeat`;
    introbg.backgroundSize = `cover`;
    setTimeout(() => {
      introbg.opacity = 1;
    }, 300)
  }
  window.onload = deferAddimage();

</script>
