      // clip-path: polygon(0% 80%, 0% -0.5%, 100% -0.5%, 100% 80%, 50% 100%);
    #bgbackdrop {
      background-color: hsl(var(--color1hsl));
      background: hsl(var(--color1hsl));
      z-index: -1;
    }
    #bgimage, .shapeintrobg, #bgbackdrop  {
      background-repeat: no-repeat;  
      background-size: cover;
      clip-path: polygon(0% 80%, 0% -0.5%, 100% -0.5%, 100% 80%, 50% 100%);
      position: absolute;
      width: 100%;
      height: 100%;
    }
    #bgimage {
      opacity: 0;
      // transition-delay: 2s;
      transition: opacity 4s 0.2s;
    }
    .shapeintrobg {
      background: linear-gradient(172deg, hsl(var(--color1hsl)) 20%, hsla(var(--color1h), 70%, 20%, 15%) );
    }
    & #productintroscreen{
      position: absolute;
      max-width: 55rem;
      bottom: -9rem;
      max-width: 80%;
      box-shadow:none;

      left: 50%;
      transform-origin: 50% 50%;
      transform: translate3d(-50%, 0, 0);
      
      z-index: 300;
      // box-shadow: 0 12px 20px -4px #111;
      border-radius: 12px;
      aspect-ratio: 16 / 9;
      & img {

      }
    }
    .introtext {
      position: absolute;
      width: 100%;
      padding-inline: 2rem;
      // left: 8vw;
      top: -10px;
      z-index: 100;

      & h2, & p, & h3 {
        color: white;
        text-align: center;
        width: 100%;
      }
      & > h2 {
        line-height: 3rem;
        font-size: 2.4rem;
        font-weight: bolder;
        text-indent: 0;
        position: relative;
        top: 2rem;
        margin: 0 0 2rem;
        display: block;
        text-shadow: 0 4px 12px #000000aa;
      }
      & > p {
        margin-top: 4rem;
        font-size: 110%;
        text-indent: 0;
        padding-right: 5%;
        text-shadow: 0 3px 9px  #44444499;
      }
      & > h3 {
        margin-top: 1.5rem;
        font-size: 1.3rem;
        letter-spacing: 0;
        font-weight: bolder;
        text-shadow: 0 2px 4px #66666699;
      }
    }
  }
  
  #testscreen {
    height: 80vw;
  }
  @media (max-width: 446px ){
    .shapeintro > .introtext {
      width: 90%;
      // bottom: calc(20vw + 12rem);
    }
  }
  @media (min-width: 1110px ){
    .shapeintro {
      // margin-bottom: 21vw;
    }
    .shapeintro > .introtext {
      bottom: calc(22vw * 0.80 + 14rem);
    }
  }
  
  @media (min-width:669px){
    .shapeintro {
  
