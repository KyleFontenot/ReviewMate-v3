---
import HomepageLayout from '../layouts/HomepageLayout.astro'
import FlexDiv from '../components/FlexDiv.astro'
import Button from "../components/Button.astro"
import CallToAction from "../components/CallToAction.astro" 
import Stream from "../components/Stream.astro" 
import ConcernBlob from '../components/ConcernBlob.astro'
import RMDiagram from '../components/RMDiagram.astro'
import ChangeTracker from '../components/ChangeTracker.astro'
import Timeline from '../components/Timeline.astro'
import SmallLink from '../components/SmallLink.astro'
import {Img} from "astro-imagetools/components"
import {formatSlug} from '../utils'
