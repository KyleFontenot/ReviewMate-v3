---
import StandardLayout from '../layouts/StandardLayout.astro'
import Stream from '../components/Stream.astro'
import {pageOpenGraph} from '../utils.js'
---
<StandardLayout 
  openGraph={new pageOpenGraph('Relevant External Articles', 
  "In a constantly changing ecosystem of medical health information, ReviewMate stands as a source of excellent articles particuarly relevant to the software and its adjacent usage.")} canonical={Astro.url}>
  <Stream postType="articlePost" size="large" title="Relevant External Articles"/>
</StandardLayout>