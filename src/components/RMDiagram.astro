<div class="RMDiagram" style="max-width: 60rem;">
  <h3 style="top: 1rem;text-align: center;display:block;">ReviewMate solves these common industry problems with: </h3>
  <div class="diagrampara" id="paragraph1" style="top: 15%;left: 0%;">
    <h4>Consolidated forms instead of disparate spreadsheets.</h4> 
    <ul>
      <li>Clean, concise organization of account data</li>
      <li>Easy navigation between accounts</li>
      <li>Groupable sets of relevant accounts</li>
    </ul>
  </div>
  <div class="diagrampara" id="paragraph2" style="top: 15%;right: 0%;">
    <h4>Synchronized and centralized communication between all parties.</h4> 
    <ul>
      <li>Simplified, modern communication with traceable to each party.</li>
      <li>No need for switching between other services.</li>
      <li>Email directly from within ReviewMate</li>
    </ul>
  </div>
  <div class="diagrampara" id="paragraph3" style="bottom: 25%;left: 0%;">
    <h4>Complete narratives for each review.</h4> 
    <ul>
      <li>Transparent, chronologic timelines</li>
      <li>Full insight into coders' decisions like physician queries.</li>
    </ul>
  </div>
  <div class="diagrampara" id="paragraph4" style="bottom: 25%;right: 0%;">
    <h4>Trackable tasks that relay progress.</h4> 
    <ul>
      <li>Keeps jobs grouped into sets of findings or accounts</li>
      <li>Actively tracks progress on each job</li>
      <li>Updates information to every relevant party including coders and clients</li>
    </ul>
  </div>
<style lang="scss">
  .RMDiagram {
    position:relative;
    margin-inline: auto;
    max-width: 70rem;
    margin-top:3rem;
    width: clamp(20rem, 104%, 70rem);
  }
  @media (max-width: 570px){
    .RMDiagram svg {
      display: none;
    }
  }
.diagrampara {
  position: absolute;
  background-color: white;
  border-radius: 10px;
  padding: 1.5rem;
  box-shadow:0 4px 18px -6px #333333;
  text-indent: 0;
  opacity: 0;
  transition: opacity 0.4s;
  width: clamp( 8rem, calc(50% - 2.5rem), 40rem);
  & h3 {
    text-align:center;
  }
  & li {
    margin-bottom: 0.3rem;
  }
}
</style>
<script>
  const MEDIA571 = window.matchMedia("(min-width: 571px)")
    let windowWidth = window.innerWidth;
    const animationEnum = {
        1: "topLeft",
        2: "topRight",
        3: "bottomLeft",
        4: "bottomRight",
      }
    const translateEnum = {
      0: 'translate(0, 0)',
      1: 'translate(-10px, -10px)',
      2: 'translate(10px, -10px)',
      3: 'translate(-10px, 10px)',
      4: 'translate(10px, 10px)',
    }
    window.onresize = () => {
      windowWidth = window.innerWidth
      checkMediaWidth()
    };
    function checkMediaWidth() {
      if(MEDIA571.matches){
        let hovering = {
          enum: 4,
          timer: true,
        };
        function activateAnim(number){
          let curve = document.querySelector(`#curve${number}`)
          let paragraph = document.querySelector(`#paragraph${number}`)
          paragraph.style.opacity = "1"
          curve.style.animationName = animationEnum[number].concat('Hover');
          curve.style.transform = translateEnum[number];
        }
        function deactivateAnim(number){
          let curve = document.querySelector(`#curve${number}`)
          let paragraph = document.querySelector(`#paragraph${number}`)
          paragraph.style.opacity = "0"
          curve.style.animationName = animationEnum[number];
          curve.style.transform = translateEnum[0];
        }

        function assignMouseEvents (number) {
          let curve = document.querySelector(`#curve${number}`)
          let paragraph = document.querySelector(`#paragraph${number}`)
          curve.addEventListener("mouseover", (e) => {
            deactivateAll()
            activateAnim(number);
            hovering = {
              enum: number,
              timer: false
            };
          }) 
          curve.addEventListener("mouseout", (e) => {
            deactivateAnim(number);
            hovering = {
              enum: number,
              timer: true
            };
            checkAuto()
          }) 
          paragraph.addEventListener("mouseover", (e) => {
            deactivateAll()
            activateAnim(number)
            hovering = {
              enum: number,
              timer: false
            };
          }) 
          paragraph.addEventListener("mouseout", (e) => {
            deactivateAnim(number)
            hovering = {
              enum: number,
              timer: true
            };
            checkAuto()
          }) 
        }
      document.querySelectorAll('.circlesegment').forEach((each, index) => {
        assignMouseEvents(index + 1)
      })

      function deactivateAll() {
        deactivateAnim(1)
        deactivateAnim(2)
        deactivateAnim(3)
        deactivateAnim(4)
      }

      let currentActive = 1;
      let observer = new IntersectionObserver(checkAuto, {
        root: null,
        rootMargin: '20px',
        threshold: 1.0
      });
      document.querySelector('#RMDiagram') && observer.observe(document.querySelector('#RMDiagram'))

      let ticker;
      let determineTimer;
      function checkAuto() {
        let timer = 0;
        setTimeout(() => {
          if(hovering.timer){
            activateAnim(hovering.enum)
          }
        }, 200)
        clearInterval(ticker)
        clearInterval(determineTimer)
        ticker = setInterval(() => {
          timer += 300
        }, 300);
        determineTimer = setInterval(() => {
          if(timer > 2000 && hovering.timer){
            deactivateAll()
            if(hovering.enum === 4) {
              activateAnim(1)
              hovering.enum = 1
            }
            else {
              activateAnim(hovering.enum + 1)
              hovering.enum++
            }
            timer = 0;
          }
        }, 800);
      }
      }
      else {
        let hovering = {
          enum: 4,
          timer: true,
        };
      }
    }
    checkMediaWidth()
  </script>
<svg id="RMDiagram" width="100%" height="100%" viewBox="0 0 750 600" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" xml:space="preserve" xmlns:serif="http://www.serif.com/" style="margin-inline:auto;fill-rule:evenodd;clip-rule:evenodd;stroke-linejoin:round;stroke-miterlimit:2;margin-bottom:3rem;">
  
  <style>
    .shadow {
      fill: #333333aa;
      filter: drop-shadow(0px 5px 5px #333333aa) blur(3px);
      opacity: 0.5;
    }
    .circlesegment {
      fill: rgb(174,62,56);
      /* fill: rgb(255,255,255); */
      filter: drop-shadow(0px 5px 2px #333333aa);
      stroke: #ffffff00;
      stroke: rgb(184,72,66);
      stroke-width: 2px;
      transform-origin: 50% 50%;
      transition: transform: 0.4s;
    }
    #curve1, #curve2, #curve3, #curve4{
      animation-duration: 1s;
      animation-iteration-count: 1;
      animation-timing-function: ease;
    }
    #curve1 {animation-name: topLeft;}
    #curve2 {animation-name: topRight;}
    #curve3 {animation-name: bottomLeft;}
    #curve4 {animation-name: bottomRight;}
    #paragraph1:hover #curve1, #curve1:hover  {
      animation-name: topLeftHover;
      transform: translate(-10px, -10px);
    }
    #paragraph2:hover #curve2, #curve2:hover {
      animation-name: topRightHover;
      transform: translate(10px, -10px);
    }
    #paragraph3:hover #curve3, #curve3:hover  {
      animation-name: bottomLeftHover;
      transform: translate(-10px, 10px);
    }
    #paragraph4:hover #curve4, #curve4:hover {
      animation-name: bottomRightHover;
      transform: translate(10px, 10px);
    }

    @keyframes topLeftHover {
      from {
        transform: translate(0, 0);
      }
      to {
        transform: translate(-10px, -10px);
      }
    }
    @keyframes topLeft {
      from {
        transform: translate(-10px, -10px);
      }
      to {
        transform: translate(0, 0);
      }
    }
    @keyframes topRightHover {
      from {
        transform: translate(0, 0);
      }
      to {
        transform: translate(10px, -10px);
      }
    }
    @keyframes topRight {
      from {
        transform: translate(10px, -10px);
      }
      to {
        transform: translate(0, 0);
      }
    }
    @keyframes bottomLeftHover {
      from {
        transform: translate(0, 0);
      }
      to {
        transform: translate(-10px, 10px);
      }
    }
    @keyframes bottomLeft {
      from {
        transform: translate(-10px, 10px);
      }
      to {
        transform: translate(0, 0);
      }
    }
    @keyframes bottomRightHover {
      from {
        transform: translate(0, 0);
      }
      to {
        transform: translate(10px, 10px);
      }
    }
    @keyframes bottomRight {
      from {
        transform: translate(10px, 10px);
      }
      to {
        transform: translate(0, 0);
      }
    }
  </style>


