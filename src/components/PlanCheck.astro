---
  const {bool} = Astro.props;
---

  {bool ? <img src="/icons/checkmark.svg" aria-label="Yes" role="img" alt="Featured" style="width: 1.5rem; height:1.5rem;filter: invert(90%) sepia(8%) saturate(1894%) hue-rotate(42deg) brightness(92%) contrast(99%);" /> :
  
  <img src="/icons/nomark.svg" aria-label="No" role="img" alt="Not featured" style="width: 1.5rem; height:1.5rem;filter: invert(23%) sepia(37%) saturate(3007%) hue-rotate(343deg) brightness(121%) contrast(88%);" />}
