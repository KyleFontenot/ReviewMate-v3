---
import DocumentLayout from "./DocumentLayout.astro"
import Header from "../components/Header.astro"
import Footer from "../components/Footer.astro"
import {pageOpenGraph} from '../utils.js'
const {slug} = Astro.props;

let openGraph = Astro.props.openGraph || new pageOpenGraph(slug, "An all-in-one software platform for medical code auditing. Streamline medical code auditing workflows by expediting findings, integrating communication, maintaining fidelity, and optimizing workflow through automation.")
---

<DocumentLayout {openGraph}>
  <Header/>
  <main id="main"><slot /></main>
    
  <Footer/>
</DocumentLayout>